#Brute Force Approach 
Time Complexity=O(n*n) Space Complexity O(1)
public class Solution {
    public static int team(int []arr, int n){
       int count=0;
       for(int i=0;i<n;i++){
           for(int j=i+1;j<n;j++){
               if(arr[i]>(arr[j]*2))
               count++;
           }
       }
       return count;
    }
}
